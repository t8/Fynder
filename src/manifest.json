  {
    "name": "Fynder",
    "version": "1.0",
    "description": "Automatically finds the top vocabulary words in any article of your choice!",
    "icons": {
      "16": "/static/images/logo_16.png",
      "32": "/static/images/logo_32.png",
      "64": "/static/images/logo_64.png",
      "128": "/static/images/logo_128.png"
    },
    "permissions": [
      "activeTab",
      "tabs",
      "declarativeContent",
      "https://en.wiktionary.org/*"
    ],
    "browser_action": {
      "default_icon": "/static/images/logo_16.png",
      "default_popup": "/popups/popup.html"
    },
    "manifest_version": 2
  }