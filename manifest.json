  {
    "name": "Fynder",
    "version": "1.0",
    "description": "Automatically finds the top 10 vocabulary words in any article of your choice!",
    "icons": {
      "16": "images/logo_16.png",
      "32": "images/logo_32.png",
      "64": "images/logo_64.png",
      "128": "images/logo_128.png"
    },
    "permissions": [
      "activeTab",
      "tabs",
      "declarativeContent"
    ],
    "browser_action": {
      "default_icon": "images/logo_16.png",
      "default_popup": "popup.html"
    },
    "manifest_version": 2
  }
